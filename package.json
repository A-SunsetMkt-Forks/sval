{
  "name": "sval",
  "version": "0.3.0",
  "description": "A javascript interpreter written in javascript",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "unpkg": "dist/sval.min.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "node scripts && npm test && npm run bundle",
    "start": "node scripts && npm run bundle",
    "bundle": "rm -rf dist types && rollup -c && mv types/src/index.d.ts types && rm -rf types/src",
    "test": "jest --colors --coverage",
    "cov": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Siubaak/sval.git"
  },
  "keywords": [
    "sval",
    "js",
    "javascript",
    "eval",
    "interpreter"
  ],
  "author": "baakqiu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Siubaak/sval/issues"
  },
  "homepage": "https://github.com/Siubaak/sval#readme",
  "dependencies": {
    "acorn": "^6.1.1"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.5",
    "@types/estree": "0.0.39",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "jest": "^24.7.1",
    "rollup": "^1.10.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
